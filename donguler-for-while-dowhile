<?php
echo "<table width=200 border=10 cellpadding=10  bordercolor=#9933FF>";

for ($i=1; $i<=2; $i++) {

echo "<tr>";
if($i ==1) {
	$baslangic=1;
	$bitis=5;
} 
else {
	$baslangic=6;
	$bitis=10;
	
}
for ($j=$baslangic; $j<=$bitis; $j++) {
echo "<td bgcolor=#33FF00>";
for ($k=1; $k<=10; $k++) {

echo $j.".".$k."=".$j*$k."<br>";	
}
echo "</td>";



}
echo "</tr>";
}

echo "</table>";

echo "<table width=200 border=10 cellpadding=10  bordercolor=#9933FF>";

